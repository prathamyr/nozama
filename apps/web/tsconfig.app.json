{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "skipLibCheck": true,
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules", "**/node_modules/**","src/**/*.spec.ts"
  ]
}
